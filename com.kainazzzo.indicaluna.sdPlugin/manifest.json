{
  "$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
  "Name": "IndicaLuna",
  "Version": "1.0.0.0",
  "Author": "kainazzzo",
  "Description": "Control 3D printers via Moonraker API - send G-code, display stats",
  "Category": "IndicaLuna",
  "CategoryIcon": "imgs/category-icon",
  "Icon": "imgs/plugin-icon",
  "CodePath": "plugin.js",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.0"
  },
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion": "10.15"
    },
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Nodejs": {
    "Version": "20",
    "Debug": "enabled"
  },
  "UUID": "com.kainazzzo.indicaluna",
  "Actions": [
    {
      "Icon": "imgs/preheat-icon",
      "Name": "Preheat",
      "States": [
        {
          "Image": "imgs/preheat-key"
        }
      ],
      "SupportedInMultiActions": false,
      "Tooltip": "Send preheat G-code with bed and nozzle temperatures",
      "UUID": "com.kainazzzo.indicaluna.preheat",
      "PropertyInspectorPath": "ui/preheat.html"
    },
    {
      "Icon": "imgs/gcode-icon",
      "Name": "Custom G-code",
      "States": [
        {
          "Image": "imgs/gcode-key"
        }
      ],
      "SupportedInMultiActions": false,
      "Tooltip": "Send custom G-code commands",
      "UUID": "com.kainazzzo.indicaluna.gcode",
      "PropertyInspectorPath": "ui/gcode.html"
    },
    {
      "Icon": "imgs/display-icon",
      "Name": "Display",
      "States": [
        {
          "Image": "imgs/display-key"
        }
      ],
      "SupportedInMultiActions": false,
      "Tooltip": "Display data from HTTP requests with JSONPath and templates",
      "UUID": "com.kainazzzo.indicaluna.display",
      "PropertyInspectorPath": "ui/display.html"
    }
  ]
}
